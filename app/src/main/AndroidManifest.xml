<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dani.unishare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/unishare"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GestioneForum.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GestioneUtente.PostActivity" />
        <activity android:name=".GestioneForum.CommentiActivity" />
        <activity android:name=".GestioneUtente.RegistrazioneActivity" />
        <activity android:name=".GestioneUtente.LoginActivity" />
        <activity android:name=".GestioneUtente.ProfiloActivity" />
        <activity android:name=".GestioneUtente.ManagerActivity" />
        <activity android:name=".GestioneUtente.RicercaProfiloActivity"></activity>
    </application>

</manifest>